generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//models example
model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}